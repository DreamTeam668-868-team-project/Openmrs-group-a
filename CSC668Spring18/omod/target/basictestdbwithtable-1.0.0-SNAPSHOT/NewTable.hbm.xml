<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.basictestdbwithtable">

    <class name="NewTable"
               table="new_table">
        <id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native" />
        </id>
        <discriminator column="id" insert="false" />
        <property name="info" type="java.lang.String" column="info" ></property>             
        <property name="entryDate" type="java.util.Date" column="entry_date" ></property> 
        <property name="userId" type="int" column="user_id" ></property>                 
    </class>
</hibernate-mapping>