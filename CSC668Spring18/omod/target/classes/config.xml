<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>csc668spring18</id>
	<name>CSC 668 Spring 2018 Module</name>
	<version>1.0.0-SNAPSHOT</version>
	<package>org.openmrs.module.csc668spring18</package>
	<author>levine</author>
	<description>
		API project for CSC668Spring18
	</description>
        
        <!--
	<updateURL>https://modules.openmrs.org/modules/download/csc668spring18/update.rdf</updateURL>
        -->
	<require_version>1.11.6</require_version>
	<!-- / Module Properties -->

	<require_modules>
		<require_module version="${appframeworkVersion}">
			org.openmrs.module.appframework
		</require_module>
		<require_module version="${uiframeworkVersion}">
			org.openmrs.module.uiframework
		</require_module>
		<require_module version="${uicommonsModuleVersion}">
			org.openmrs.module.uicommons
		</require_module>
		<require_module version="${appuiVersion}">
			org.openmrs.module.appui
		</require_module>
	</require_modules>
	
	<!-- Module Activator -->
	<activator>org.openmrs.module.csc668spring18.AccessRecordActivator</activator>
	
	<aware_of_modules>
		<aware_of_module>org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>
	<!-- Adds link to admin page 
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.csc668spring18.extension.html.AdminList</class>
	</extension>
	-->
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		AccessLog.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with csc668spring18. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

