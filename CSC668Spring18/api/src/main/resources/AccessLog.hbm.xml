<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.csc668spring18">
  <class name="AccessRecord" table="access_log">
    <id column="id" name="id" type="java.lang.Integer" unsaved-value="0">
      <generator class="native"/>
    </id>
    <discriminator column="id" insert="false"/>
    <property column="accessed_on" name="accessedOn" type="java.util.Date" />
    <property column="accessing_user" name="accessingUser" type="org.openmrs.User" />
    <property column="access_location" name="accessLocation" type="org.openmrs.Location" />
    <property column="accessed_record_id" name="recordId" type="java.lang.Integer" />
    <property column="accessed_record_type" name="recordType" type="java.lang.String" />
  </class>
</hibernate-mapping>
